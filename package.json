{
	"name": "http-body-json-parse",
	"description": "Parse the JSON body of an http request",
	"version": "2.0.0",
	"repository": "srcagency/http-body-json-parse",
	"scripts": {
		"test": "node test",
		"format": "prettier --write ./*"
	},
	"keywords": [
		"http",
		"request",
		"body",
		"json",
		"parse"
	],
	"dependencies": {
		"bluebird": "^3.5.1",
		"concat-stream": "^1.6.0",
		"debug": "^3.1.0"
	},
	"devDependencies": {
		"prettier": "^1.14.3",
		"request": "^2.83.0",
		"tape": "^4.8.0"
	},
	"prettier": {
		"semi": false,
		"singleQuote": true,
		"useTabs": true,
		"bracketSpacing": false,
		"trailingComma": "es5",
		"tabWidth": 4,
		"overrides": [
			{
				"files": "*.md",
				"options": {
					"tabWidth": 2
				}
			}
		]
	},
	"license": "MIT"
}
